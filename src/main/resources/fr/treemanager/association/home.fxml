<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.*?>
<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>
<?import javafx.scene.shape.*?>
<?import javafx.scene.text.*?>

<Pane prefHeight="500.0" prefWidth="724.0" xmlns="http://javafx.com/javafx/17.0.12" xmlns:fx="http://javafx.com/fxml/1">
   <HBox alignment="CENTER" maxHeight="-Infinity" maxWidth="-Infinity" minHeight="-Infinity" minWidth="-Infinity" prefHeight="500.0" prefWidth="724.0" xmlns="http://javafx.com/javafx/23.0.1" xmlns:fx="http://javafx.com/fxml/1">
      <children>
         <VBox alignment="CENTER" prefHeight="384.0" prefWidth="201.0" HBox.hgrow="ALWAYS">
            <children>
               <Text strokeType="OUTSIDE" strokeWidth="0.0" text="Balance de l'association">
                  <font>
                     <Font size="20.0" />
                  </font></Text>
               <Text strokeType="OUTSIDE" strokeWidth="0.0" text="Vous pouvez retrouver ici la balance financière de l'association">
                  <VBox.margin>
                     <Insets top="5.0" />
                  </VBox.margin>
               </Text>
               <Text fx:id="balanceText" strokeType="OUTSIDE" strokeWidth="0.0">
                  <VBox.margin>
                     <Insets top="10.0" />
                  </VBox.margin>
                  <font>
                     <Font size="22.0" />
                  </font>
               </Text>
            </children>
         </VBox>
         <Line endY="-200.0" startY="200.0" />
         <VBox alignment="CENTER" prefHeight="400.0" prefWidth="220.0" HBox.hgrow="ALWAYS">
            <children>
               <Text strokeType="OUTSIDE" strokeWidth="0.0" text="Prochain exercice">
                  <font>
                     <Font size="20.0" />
                  </font></Text>
               <Button fx:id="nextExercize" mnemonicParsing="false" onAction="#toNextExercize" text="passer à l'exercice suivant">
                  <VBox.margin>
                     <Insets top="5.0" />
                  </VBox.margin></Button>
               <Text fx:id="budgetYearText" strokeType="OUTSIDE" strokeWidth="0.0" text="Text">
                  <font>
                     <Font name="System Italic" size="12.0" />
                  </font>
                  <VBox.margin>
                     <Insets top="5.0" />
                  </VBox.margin>
               </Text>
            </children>
         </VBox>
      </children>
   </HBox>
</Pane>
